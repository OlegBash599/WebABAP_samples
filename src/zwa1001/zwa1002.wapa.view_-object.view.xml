<mvc:View                                                                                                                                                                                                                                                      
	controllerName="zwa1002.zwa1002.controller.Object"                                                                                                                                                                                                            
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns:semantic="sap.f.semantic">                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
	<semantic:SemanticPage                                                                                                                                                                                                                                        
		id="page"                                                                                                                                                                                                                                                    
		headerPinnable="false"                                                                                                                                                                                                                                       
		toggleHeaderOnTitleClick="false"                                                                                                                                                                                                                             
		busy="{objectView>/busy}"                                                                                                                                                                                                                                    
		busyIndicatorDelay="{objectView>/delay}">                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
		<semantic:titleHeading>                                                                                                                                                                                                                                      
			<Title text="{Description}" />                                                                                                                                                                                                                              
		</semantic:titleHeading>                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
		<semantic:headerContent>                                                                                                                                                                                                                                     
			<ObjectNumber                                                                                                                                                                                                                                               
				number="{                                                                                                                                                                                                                                                  
					path: 'NumOfValues',                                                                                                                                                                                                                                      
					formatter: '.formatter.numberUnit'                                                                                                                                                                                                                        
				}"                                                                                                                                                                                                                                                         
			/>                                                                                                                                                                                                                                                          
		</semantic:headerContent>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>                                                                                                                                                                                       
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
		<semantic:content>                                                                                                                                                                                                                                           
			<Table                                                                                                                                                                                                                                                      
				id="table"                                                                                                                                                                                                                                                 
				width="auto"                                                                                                                                                                                                                                               
				items="{VarNameSet>/results}"                                                                                                                                                                                                                              
				noDataText="{worklistView>/tableNoDataText}"                                                                                                                                                                                                               
				busyIndicatorDelay="{worklistView>/tableBusyDelay}"                                                                                                                                                                                                        
				growing="true"                                                                                                                                                                                                                                             
				growingScrollToLoad="true"                                                                                                                                                                                                                                 
				updateFinished="onUpdateFinished">                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<headerToolbar>                                                                                                                                                                                                                                            
					<Toolbar>                                                                                                                                                                                                                                                 
						<Title id="tableHeader" text="{worklistView>/worklistTableTitle}"/>                                                                                                                                                                                      
						<ToolbarSpacer />                                                                                                                                                                                                                                        
						<SearchField                                                                                                                                                                                                                                             
							id="searchField"                                                                                                                                                                                                                                        
							tooltip="{i18n>worklistSearchTooltip}"                                                                                                                                                                                                                  
							search="onSearch"                                                                                                                                                                                                                                       
							width="auto">                                                                                                                                                                                                                                           
						</SearchField>                                                                                                                                                                                                                                           
					</Toolbar>                                                                                                                                                                                                                                                
				</headerToolbar>                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
				<columns>                                                                                                                                                                                                                                                  
					<Column id="nameColumn">                                                                                                                                                                                                                                  
						<Text text="{i18n>tableNameColumnTitle}" id="nameColumnTitle"/>                                                                                                                                                                                          
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarSignColumn">                                                                                                                                                                                                                               
						<Text text="{i18n>tableNameColumnTitle}" id="VarSignColumnTitle"/>                                                                                                                                                                                       
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarOptiColumn">                                                                                                                                                                                                                               
						<Text text="{i18n>tableNameColumnTitle}" id="VarOptiColumnTitle"/>                                                                                                                                                                                       
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarLowColumn">                                                                                                                                                                                                                                
						<Text text="{i18n>tableNameColumnTitle}" id="VarLowColumnTitle"/>                                                                                                                                                                                        
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarHighColumn">                                                                                                                                                                                                                               
						<Text text="{i18n>tableNameColumnTitle}" id="VarHighColumnTitle"/>                                                                                                                                                                                       
					</Column>                                                                                                                                                                                                                                                 
					<Column id="unitNumberColumn" hAlign="End">                                                                                                                                                                                                               
						<Text text="{i18n>tableUnitNumberColumnTitle}" id="unitNumberColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem                                                                                                                                                                                                                                           
						type="Navigation"                                                                                                                                                                                                                                        
						press="onPress">                                                                                                                                                                                                                                         
						<cells>                                                                                                                                                                                                                                                  
							<ObjectIdentifier                                                                                                                                                                                                                                       
								title="{VarNameSet>VarName}"/>                                                                                                                                                                                                                         
							<Text text="{VarNameSet>Sign}" />                                                                                                                                                                                                                       
							<Text text="{VarNameSet>Opti}" />                                                                                                                                                                                                                       
							<Text text="{VarNameSet>Low}" />                                                                                                                                                                                                                        
							<Text text="{VarNameSet>High}" />                                                                                                                                                                                                                       
							<ObjectNumber                                                                                                                                                                                                                                           
								number="{                                                                                                                                                                                                                                              
									path: 'VarNameSet>VarNumb'                                                                                                                                                                                                                            
								}"/>                                                                                                                                                                                                                                                   
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
			</Table>                                                                                                                                                                                                                                                    
		</semantic:content>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
	</semantic:SemanticPage>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    