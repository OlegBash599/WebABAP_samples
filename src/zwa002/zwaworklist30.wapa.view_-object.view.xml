<mvc:View                                                                                                                                                                                                                                                      
	controllerName="zwaworklist30.ZWEBABAP_WORKLIST30.controller.Object"                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns:layout="sap.ui.layout"                                                                                                                                                                                                                                  
	xmlns:semantic="sap.f.semantic">                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
	<semantic:SemanticPage                                                                                                                                                                                                                                        
		id="page"                                                                                                                                                                                                                                                    
		headerPinnable="false"                                                                                                                                                                                                                                       
		toggleHeaderOnTitleClick="false"                                                                                                                                                                                                                             
		showFooter="true"                                                                                                                                                                                                                                            
		busy="{objectView>/busy}"                                                                                                                                                                                                                                    
		busyIndicatorDelay="{objectView>/delay}">                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
		<!-- Title Breadcrumbs-->                                                                                                                                                                                                                                    
		<semantic:titleBreadcrumbs>                                                                                                                                                                                                                                  
			<Breadcrumbs>                                                                                                                                                                                                                                               
				<Link press="onNavBack" text="Back to Variable List"/>                                                                                                                                                                                                     
			</Breadcrumbs>                                                                                                                                                                                                                                              
		</semantic:titleBreadcrumbs>                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>                                                                                                                                                                                       
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
		<semantic:content>                                                                                                                                                                                                                                           
			<VBox>                                                                                                                                                                                                                                                      
				<VBox>                                                                                                                                                                                                                                                     
					<Label text="ID переменной" labelFor="VarIDfield"/>                                                                                                                                                                                                       
					<Input id="VarIDfield" value="{VarValSet>/d/Name}" editable="false"/>                                                                                                                                                                                     
				</VBox>                                                                                                                                                                                                                                                    
				<VBox>                                                                                                                                                                                                                                                     
					<Label text="Описание переменной:" labelFor="VarDescfield"/>                                                                                                                                                                                              
					<Input id="VarDescfield" value="{VarValSet>/d/Description}" editable="false"/>                                                                                                                                                                            
				</VBox>                                                                                                                                                                                                                                                    
				<VBox>                                                                                                                                                                                                                                                     
					<Label text="Тип:" labelFor="VarTypeField"/>                                                                                                                                                                                                              
					<Input id="VarTypeField" value="{VarValSet>/d/VarType}" editable="false"/>                                                                                                                                                                                
				</VBox>                                                                                                                                                                                                                                                    
				<VBox>                                                                                                                                                                                                                                                     
					<Label text="Быстрое Значение:" labelFor="VarFastValField"/>                                                                                                                                                                                              
					<Input id="VarFastValField" value="{VarValSet>/d/FastVal}" editable="false"/>                                                                                                                                                                             
				</VBox>                                                                                                                                                                                                                                                    
				<HBox>                                                                                                                                                                                                                                                     
					<CheckBox id="VarIsDelField" text="Помечен на удаление" selected="{VarValSet>/d/IsDel}" editable="false"/>                                                                                                                                                
				</HBox>                                                                                                                                                                                                                                                    
				<HBox>                                                                                                                                                                                                                                                     
					<CheckBox id="VarDebugIsOnField" text="Вкл/Выкл отладка" selected="{VarValSet>/d/DebugIsOn}" editable="false"/>                                                                                                                                           
				</HBox>                                                                                                                                                                                                                                                    
			<Table                                                                                                                                                                                                                                                      
				id="VarValTable"                                                                                                                                                                                                                                           
				width="auto"                                                                                                                                                                                                                                               
				items="{VarValSet>/d/VarID2Values/results}"                                                                                                                                                                                                                
				noDataText="{objectView>/tableNoDataText}"                                                                                                                                                                                                                 
				busyIndicatorDelay="{objectView>/tableBusyDelay}"                                                                                                                                                                                                          
				growing="true"                                                                                                                                                                                                                                             
				growingScrollToLoad="true"                                                                                                                                                                                                                                 
				updateFinished="onUpdateFinished">                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<columns>                                                                                                                                                                                                                                                  
					<Column id="VarValnameColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValnameColumnTitle}" id="VarValnameColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValnumbColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValnumbColumnTitle}" id="VarValnumbColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValSignColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValSignColumnTitle}" id="VarValSignColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValOptiColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValOptiColumnTitle}" id="VarValOptiColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValLowColumn">                                                                                                                                                                                                                             
						<Text text="{i18n>tableVarValLowColumnTitle}" id="VarValLowColumnTitle"/>                                                                                                                                                                                
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValHighColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValHighColumnTitle}" id="VarValHighColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem>                                                                                                                                                                                                                                          
						<cells>                                                                                                                                                                                                                                                  
							<ObjectIdentifier title="{VarValSet>VarName}"/>                                                                                                                                                                                                         
							<Text text="{VarValSet>VarNumb}" />                                                                                                                                                                                                                     
							<Text text="{VarValSet>Sign}" />                                                                                                                                                                                                                        
							<Text text="{VarValSet>Opti}" />                                                                                                                                                                                                                        
							<Text text="{VarValSet>Low}" />                                                                                                                                                                                                                         
							<Text text="{VarValSet>High}" />                                                                                                                                                                                                                        
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
			</Table>                                                                                                                                                                                                                                                    
			</VBox>                                                                                                                                                                                                                                                     
		</semantic:content>                                                                                                                                                                                                                                          
		                                                                                                                                                                                                                                                             
		<semantic:addAction id="addActionButtonTag" visible="false">                                                                                                                                                                                                 
			<semantic:AddAction id="AddActionButtonCtrl" visible="false" press="onCreateRecord"/>                                                                                                                                                                       
		</semantic:addAction>                                                                                                                                                                                                                                        
		                                                                                                                                                                                                                                                             
		<semantic:editAction  id="editActionButtonTag" visible="false">                                                                                                                                                                                              
			<semantic:EditAction id="EditActionButtonCtrl" visible="false" press="onUpdateRecord" />                                                                                                                                                                    
		</semantic:editAction>                                                                                                                                                                                                                                       
		                                                                                                                                                                                                                                                             
		<semantic:deleteAction id="deleteActionButtonTag" visible="false">                                                                                                                                                                                           
			<semantic:DeleteAction id="DeleteActionButtonCtrl" visible="false"  press="onDeleteRecord"/>                                                                                                                                                                
		</semantic:deleteAction>                                                                                                                                                                                                                                     
		                                                                                                                                                                                                                                                             
		<!-- Custom Footer Content-->                                                                                                                                                                                                                                
		<semantic:footerCustomActions>                                                                                                                                                                                                                               
			<Button text="Режим просмотра" press="ShowMode" />                                                                                                                                                                                                          
			<Button text="Режим редактирования" press="EditMode"/>                                                                                                                                                                                                      
			<Button text="Batch Operation Sample" press="BatchRequestCall"/>                                                                                                                                                                                            
		</semantic:footerCustomActions>                                                                                                                                                                                                                              
		                                                                                                                                                                                                                                                             
	</semantic:SemanticPage>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    