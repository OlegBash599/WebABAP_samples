<mvc:View                                                                                                                                                                                                                                                      
	controllerName="ZWA020.ZWEBABAP_WORKLIST20.controller.Worklist"                                                                                                                                                                                               
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns:semantic="sap.f.semantic">                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
	<semantic:SemanticPage                                                                                                                                                                                                                                        
		id="page"                                                                                                                                                                                                                                                    
		headerPinnable="false"                                                                                                                                                                                                                                       
		toggleHeaderOnTitleClick="false">                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
		<semantic:titleHeading>                                                                                                                                                                                                                                      
			<Title text="{i18n>worklistTitle}" />                                                                                                                                                                                                                       
		</semantic:titleHeading>                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
		<semantic:content>                                                                                                                                                                                                                                           
			<Table                                                                                                                                                                                                                                                      
				id="table"                                                                                                                                                                                                                                                 
				width="auto"                                                                                                                                                                                                                                               
				items="{                                                                                                                                                                                                                                                   
					path: '/VarHeadSet',                                                                                                                                                                                                                                      
					sorter: {                                                                                                                                                                                                                                                 
						path: 'Description',                                                                                                                                                                                                                                     
						descending: false                                                                                                                                                                                                                                        
					}                                                                                                                                                                                                                                                         
				}"                                                                                                                                                                                                                                                         
				noDataText="{worklistView>/tableNoDataText}"                                                                                                                                                                                                               
				busyIndicatorDelay="{worklistView>/tableBusyDelay}"                                                                                                                                                                                                        
				growing="true"                                                                                                                                                                                                                                             
				growingScrollToLoad="true"                                                                                                                                                                                                                                 
				updateFinished="onUpdateFinished">                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<headerToolbar>                                                                                                                                                                                                                                            
					<Toolbar>                                                                                                                                                                                                                                                 
						<Title id="tableHeader" text="{worklistView>/worklistTableTitle}"/>                                                                                                                                                                                      
						<ToolbarSpacer />                                                                                                                                                                                                                                        
						<SearchField                                                                                                                                                                                                                                             
							id="searchField"                                                                                                                                                                                                                                        
							tooltip="{i18n>worklistSearchTooltip}"                                                                                                                                                                                                                  
							search="onSearch"                                                                                                                                                                                                                                       
							width="auto">                                                                                                                                                                                                                                           
						</SearchField>                                                                                                                                                                                                                                           
					</Toolbar>                                                                                                                                                                                                                                                
				</headerToolbar>                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
				<columns>                                                                                                                                                                                                                                                  
					<Column id="nameColumn">                                                                                                                                                                                                                                  
						<Text text="{i18n>tableNameColumnTitle}" id="nameColumnTitle"/>                                                                                                                                                                                          
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarIDColumn">                                                                                                                                                                                                                                 
						<Text text="{i18n>tableVarIDColumnTitle}" id="VarIDColumnTitle"/>                                                                                                                                                                                        
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarTypeColumn">                                                                                                                                                                                                                               
						<Text text="{i18n>tableVarTypeColumnTitle}" id="VarTypeColumnTitle"/>                                                                                                                                                                                    
					</Column>                                                                                                                                                                                                                                                 
					<Column id="unitNumberColumn" hAlign="Center">                                                                                                                                                                                                            
						<Text text="{i18n>tableUnitNumberColumnTitle}" id="unitNumberColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="NumOfFilesColumn" hAlign="Center">                                                                                                                                                                                                            
						<Text text="{i18n>tableNumOfFilesColumnTitle}" id="NumOfFilesColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarFastValColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarFastValColumnTitle}" id="VarFastValColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarIsDelColumn">                                                                                                                                                                                                                              
						<Text text="{i18n>tableVarIsDelColumnTitle}" id="VarIsDelColumnTitle"/>                                                                                                                                                                                  
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarDebugIsOnColumn">                                                                                                                                                                                                                          
						<Text text="{i18n>tableVarDebugIsOnColumnTitle}" id="VarDebugIsOnColumnTitle"/>                                                                                                                                                                          
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarChdColumn">                                                                                                                                                                                                                                
						<Text text="{i18n>tableVarChdColumnTitle}" id="VarChdColumnTitle"/>                                                                                                                                                                                      
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem                                                                                                                                                                                                                                           
						type="Navigation"                                                                                                                                                                                                                                        
						press="onPress">                                                                                                                                                                                                                                         
						<cells>                                                                                                                                                                                                                                                  
							<ObjectIdentifier title="{Description}"/>                                                                                                                                                                                                               
							<Text text="{Name}" />                                                                                                                                                                                                                                  
							<Text text="{VarType} - {VarTypeTx}" />                                                                                                                                                                                                                 
							<ObjectNumber number="{                                                                                                                                                                                                                                 
									path: 'NumOfValues',                                                                                                                                                                                                                                  
									type: 'sap.ui.model.type.Integer'}"/>                                                                                                                                                                                                                 
							<ObjectNumber number="{                                                                                                                                                                                                                                 
									path: 'NumOfFiles',                                                                                                                                                                                                                                   
									type: 'sap.ui.model.type.Integer'}"/>                                                                                                                                                                                                                 
							<Text text="{FastVal}" />                                                                                                                                                                                                                               
							<CheckBox  selected="{IsDel}" valueState="Warning" enabled="false" />                                                                                                                                                                                   
							<CheckBox  selected="{DebugIsOn}" valueState="Success" enabled="false" />                                                                                                                                                                               
							<Text text="{                                                                                                                                                                                                                                           
								path: 'Chd',                                                                                                                                                                                                                                           
								type: 'sap.ui.model.type.Date',                                                                                                                                                                                                                        
								formatOptions: {&nbsp;                                                                                                                                                                                                                                 
										pattern: 'yyyy/MM/dd'&nbsp;                                                                                                                                                                                                                          
										}                                                                                                                                                                                                                                                    
								}"/>                                                                                                                                                                                                                                                   
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
			</Table>                                                                                                                                                                                                                                                    
		</semantic:content>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>                                                                                                                                                                                       
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
	</semantic:SemanticPage>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    