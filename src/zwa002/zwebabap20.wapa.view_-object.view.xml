<mvc:View                                                                                                                                                                                                                                                      
	controllerName="ZWA020.ZWEBABAP_WORKLIST20.controller.Object"                                                                                                                                                                                                 
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns:semantic="sap.f.semantic">                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
	<semantic:SemanticPage                                                                                                                                                                                                                                        
		id="page"                                                                                                                                                                                                                                                    
		headerPinnable="false"                                                                                                                                                                                                                                       
		toggleHeaderOnTitleClick="false"                                                                                                                                                                                                                             
		busy="{objectView>/busy}"                                                                                                                                                                                                                                    
		busyIndicatorDelay="{objectView>/delay}">                                                                                                                                                                                                                    
		                                                                                                                                                                                                                                                             
		<!-- Title Breadcrumbs-->                                                                                                                                                                                                                                    
		<semantic:titleBreadcrumbs>                                                                                                                                                                                                                                  
			<Breadcrumbs>                                                                                                                                                                                                                                               
				<Link press="onNavBack" text="Back to Variable List"/>                                                                                                                                                                                                     
			</Breadcrumbs>                                                                                                                                                                                                                                              
		</semantic:titleBreadcrumbs>                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>                                                                                                                                                                                       
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
		                                                                                                                                                                                                                                                             
		<semantic:content>                                                                                                                                                                                                                                           
			<Table                                                                                                                                                                                                                                                      
				id="VarValTable"                                                                                                                                                                                                                                           
				width="auto"                                                                                                                                                                                                                                               
				items="{VarRangeSet>/results}"                                                                                                                                                                                                                             
				noDataText="{objectView>/tableNoDataText}"                                                                                                                                                                                                                 
				busyIndicatorDelay="{objectView>/tableBusyDelay}"                                                                                                                                                                                                          
				growing="true"                                                                                                                                                                                                                                             
				growingScrollToLoad="true"                                                                                                                                                                                                                                 
				updateFinished="onUpdateFinished">                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<columns>                                                                                                                                                                                                                                                  
					<Column id="VarValnameColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValnameColumnTitle}" id="VarValnameColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValnumbColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValnumbColumnTitle}" id="VarValnumbColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValSignColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValSignColumnTitle}" id="VarValSignColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValOptiColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValOptiColumnTitle}" id="VarValOptiColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValLowColumn">                                                                                                                                                                                                                             
						<Text text="{i18n>tableVarValLowColumnTitle}" id="VarValLowColumnTitle"/>                                                                                                                                                                                
					</Column>                                                                                                                                                                                                                                                 
					<Column id="VarValHighColumn">                                                                                                                                                                                                                            
						<Text text="{i18n>tableVarValHighColumnTitle}" id="VarValHighColumnTitle"/>                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem>                                                                                                                                                                                                                                          
						<cells>                                                                                                                                                                                                                                                  
							<ObjectIdentifier title="{VarRangeSet>VarName}"/>                                                                                                                                                                                                       
							<Text text="{VarRangeSet>VarNumb}" />                                                                                                                                                                                                                   
							<Text text="{VarRangeSet>Sign}" />                                                                                                                                                                                                                      
							<Text text="{VarRangeSet>Opti}" />                                                                                                                                                                                                                      
							<Text text="{VarRangeSet>Low}" />                                                                                                                                                                                                                       
							<Text text="{VarRangeSet>High}" />                                                                                                                                                                                                                      
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
			</Table>                                                                                                                                                                                                                                                    
		</semantic:content>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
	</semantic:SemanticPage>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    